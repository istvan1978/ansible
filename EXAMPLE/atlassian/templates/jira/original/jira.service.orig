[Unit]
Description=Jira Service
After=network.target postgresql.service

[Service]
Type=forking
User=root
Environment=JRE_HOME=/export/java/jdk
ExecStart=/export/jira/jira/bin/start-jira.sh
ExecStop=/export/jira/jira/bin/stop-jira.sh
ExecReload=/export/jira/jira/bin/stop-jira.sh | sleep 60 | /export/jira/jira/bin/start-jira.sh
[Install]

WantedBy=multi-user.target
